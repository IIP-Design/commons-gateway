---
import PartnerPageLayout from '../layouts/PartnerPageLayout.astro';
import TableContainer from '../layouts/TableContainer.astro';
import UploaderTable from '../components/UploaderTable';

const description = 'The users listed on this page are the external partner\'s support staff who will help provide content to be utilized by Department of State.';
const title = 'External Partner Support Staff';
---

<PartnerPageLayout title={title}>
  <TableContainer btnText="+ Invite User" btnHref="newUser" desc={description} title={title}>
    <UploaderTable client:only />
  </TableContainer>
</PartnerPageLayout>
