---
import TablePageLayout from '../layouts/TablePageLayout.astro';
import UserTable from '../components/UserTable';

import { mockSomeData } from '../../mocks';

const { teams, users } = mockSomeData(100, 5);

const description = `The users listed on this page are the team leads of Content Commons external partners. These
    team leads help manage the external partner(s) who help provide content to be utilized by
    Department of State PD personnel.`;
---

<TablePageLayout btnText="+ New User" desc={description} title="External Partner Team Leads">
  <UserTable client:load teams={teams} users={users} />
</TablePageLayout>
