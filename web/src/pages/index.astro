---
import AdminPageLayout from '../layouts/AdminPageLayout.astro';
import TableContainer from '../layouts/TableContainer.astro';
import ExternalPartnerTables from '../components/ExternalPartnerTables/ExternalPartnerTables';

const description = `The users listed on this page are Content Commons external partners. Use the dropdown menu below to choose between external partner and external team leads. External partners can upload files to Content Commons. External team leads have the added ability to propose additional external partners for approval by Department of State personnel.`;
const title = 'External Partners';
---

<AdminPageLayout title={title}>
  <TableContainer btnText="+ New User" btnHref="new-user" desc={description} title={title}>
    <ExternalPartnerTables client:only />
  </TableContainer>
</AdminPageLayout>
