{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Update Team Event Body Schema",
  "description": "Data required to update an existing team",
  "type": "object",
  "properties": {
    "active": {
      "type": "boolean"
    },
    "team": {
      "type": "string",
      "minLength": 1
    },
    "teamAprimo": {
      "type": "string",
      "minLength": 1
    },
    "teamName": {
      "type": "string",
      "minLength": 1
    }
  },
  "required": ["active", "team", "teamAprimo", "teamName"],
  "additionalProperties": false
}
